
public class hora {

	private int hora;
	private int minuto;
	private int segundo;
	
	hora(){
		hora=00;
		minuto=00;
		segundo=00;
	}

	public int getHora() {
		return hora;
	}

	public void setHora(int hora) {
		this.hora = hora;
	}

	public int getMinuto() {
		return minuto;
	}

	public void setMinuto(int minuto) {
		this.minuto = minuto;
	}

	public int getSegundo() {
		return segundo;
	}

	public void setSegundo(int segundo) {
		this.segundo = segundo;
	}
	
	public int total() {
		for(segundo=0; segundo<=59; ) {
			
		}
	}
	
}
